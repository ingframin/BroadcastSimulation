   "source": [
    "from scipy.optimize import fsolve\n",
    "Trx = 60\n",
    "Ttx = 21\n",
    "Tn = 100\n",
    "\n",
    "def equations(p):\n",
    "    x, y, z = p\n",
    "    return (Trx*x/(Trx*x+Ttx*z +Tn*y), Tn*y/(Trx*x+Ttx*z +Tn*y),Ttx*z/(Trx*x+Ttx*z+Tn*y))\n",
    "\n",
    "x, y, z =  fsolve(equations, (100,100 ,100))\n",
    "print(x/(x+y+z))\n",
    "print(y/(x+y+z))\n",
    "print(z/(x+y+z))"
   ]
